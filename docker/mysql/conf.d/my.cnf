[mysqld]
general-log = On
general-log-file = /var/log/mysql/mysql.log
character-set-server=utf8
innodb_file_format = Barracuda
innodb_file_per_table = 1
innodb_large_prefix
innodb_strict_mode=0
default-time-zone = 'Asia/Tokyo'

[mysql]
default-character-set=utf8

[client]
default-character-set=utf8
